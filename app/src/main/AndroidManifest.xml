<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.franciscorojas.appfrojas">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_tech"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.CreateAccountActivity"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".view.HomeActivity"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".view.DetailActivity"
            android:parentActivityName=".view.HomeActivity" />
        <activity
            android:name=".view.DetailMacActivity"
            android:parentActivityName=".view.HomeActivity" />
        <activity
            android:name=".view.ProfileActivity"
            android:parentActivityName=".view.HomeActivity" />
        <activity
            android:name=".view.ComparePcsActivity"
            android:parentActivityName=".view.HomeActivity" />
        <activity
            android:name=".view.HelpActivity"
            android:parentActivityName=".view.PostRegisterActivity" />
        <activity
            android:name=".view.PostRegisterActivity"
            android:parentActivityName=".view.CreateAccountActivity" />
        <activity android:name=".EndActivity"></activity>
    </application>

</manifest>